                    "branch": "v0.2.57-calliope-1.5",
                    "type": "git"
                },
                "codalBinary": "MICROBIT",
                "githubCorePackage": "lancaster-university/microbit-v2-samples",
                "gittag": "v0.2.13",
                "serviceId": "mbcodal2",
                "dockerImage": "pext/yotta:update-yotta3",
                "yottaConfigCompatibility": true
            }
        }
    },
